public class test {
    public static int add(int a,int b){
        return a+b;
    }
    public static double add(double a,double b){
        return a+b;
}
    public static void main(String[] args) {
        //在同一个类中定义多个方法：要求不仅可以求2个整数的最大值，还可以求3个小数的最大值？
        int a=1;
        int b=3;
        int ret=add(a,b);
        System.out.println(ret);
        double x=2.5;
        double y=3.5;
        double sum=add(x,y);
        System.out.println(sum);
    }
}
