import java.util.Scanner;
public class test {
    public static int fac(int n){
        if(n==1){
            return 1;
        }
        return n*fac(n-1);
    }
    public static void main(String[] args) {
        Scanner scanner=new Scanner(System.in);
        //递归求 N 的阶乘
        int n=scanner.nextInt();
        int ret=fac(n);
        System.out.println(ret);
    }
}
