public int hammingWeight(int n) {
        int count=0;//计数器
        for(int i=31;i>=0;i--){
            if(((n>>i)&1)==1){
                count++;
            }
        }
        return count;
        
    } 
int hammingWeight(uint32_t n) {
       int count = 0;
        while (n) {
            n &= n - 1;
            count++;
        }
        return count;

}
